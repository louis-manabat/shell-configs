[alias]
    a = add
    aa = add .
    cm = commit -m
    p = push
    rsh = reset --soft HEAD~1
    usa = restore --staged .
    us = restore --staged
    pa = "!f() { git add -A && git commit -m \"$1\" && git push; }; f"
    nb = "!git checkout -b $1 && git push -u origin $1"
    s = status